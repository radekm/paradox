fof(a_0, axiom, lives(sk_x00)).
fof(a_1, axiom, killed(sk_x00,agatha)).
fof(a_2, axiom, lives(agatha)).
fof(a_3, axiom, ![X] : (X = charles | X = agatha | X = butler | ~lives(X))).
fof(a_4, axiom, ![Y] : ![X] : (hates(X,Y) | ~killed(X,Y))).
fof(a_5, axiom, ![Y] : ![X] : (~killed(X,Y) | ~richer(X,Y))).
