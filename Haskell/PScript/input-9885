fof(a_5, axiom, ![X] : (~c2(X) | ![Y] : (d1(f(X,Y)) | d2(f(X,Y)) | ~c2(Y)))).
fof(a_6, axiom, ![X] : (~d1(X) | ![Y] : (c1(f(X,Y)) | c2(f(X,Y)) | ~d1(Y)))).
